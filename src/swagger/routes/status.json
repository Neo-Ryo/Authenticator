{
    "get": {
        "summary": "GET the status of the service",
        "tags": ["Services"],
        "description": "Get service status",
        "operationId": "Status",
        "responses": {
            "200": {
                "description": "Service ok",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "../schemas/responseStatus.json"
                        },
                        "examples": {
                            "Service ok": {
                                "value": {
                                    "code": 200,
                                    "message": "ok"
                                }
                            }
                        }
                    }
                }
            },
            "500": {
                "description": "Service error",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "../schemas/responseStatus.json"
                        },
                        "examples": {
                            "Service Error": {
                                "value": {
                                    "code": 500,
                                    "message": "Some error message"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}