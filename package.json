{
  "name": "boilerplate_nodejs",
  "version": "1.0.0",
  "description": "Simple boilerplate with prettier and eslint setup",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project ./",
    "start": "nodemon -L index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "husky:setup": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node": "^14.17.1",
    "winston": "^3.3.3"
  },
  "keywords": [],
  "author": ""
}
